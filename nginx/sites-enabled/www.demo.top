# Default server configuration
#
server {
	listen 80;
	listen [::]:80;
	root /home/utilisateurdemo/site/;
	index index.php index.html;
	server_name www.demo.top;
	location / {
		try_files $uri $uri/ =404;
	}
	#access_log /home/utilisateurdemo/access.log;
	#error_log /home/utilisateurdemo/error.log;
	location ~ \.php$ {
		include snippets/fastcgi-php.conf;
		fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
	}
}
